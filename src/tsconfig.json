{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "../JS",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
